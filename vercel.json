{
  "builds": [
    {
      "src": "/dist/apps/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/apps/client/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/apps/server/main.js",
      "methods": ["GET", "POST"]
    }
  ]
}
