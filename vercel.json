{
  "builds": [
    {
      "src": "/apps.server/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/apps/client/*",
      "use": "@now/static"
    }
  ],
  "build": {
    "env": {
      "BUILD_SCRIPT": "npm run log-client-path && npm run build"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps.server/src/main.ts",
      "methods": ["GET", "POST"]
    }
  ]
}
